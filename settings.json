{
    // Common
    "editor.fontFamily": "Consolas, 微软雅黑",
    "editor.fontLigatures": true,
    "editor.fontSize": 16,
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 10000,
    "editor.smoothScrolling": true,
    "editor.cursorBlinking": "smooth",
    "editor.cursorSmoothCaretAnimation": true,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "explorer.confirmDelete": false,
    "files.eol": "\n",
    "files.autoGuessEncoding": true,
    "workbench.startupEditor": "none",
    "window.restoreWindows": "none",
    "debug.toolBarLocation": "docked",
    "telemetry.telemetryLevel": "off",
    "editor.renderLineHighlight": "all",
    "workbench.enableExperiments": false,
    "workbench.list.smoothScrolling": true,
    "workbench.colorTheme": "Solarized Dark",
    "workbench.iconTheme": "material-icon-theme",
    "workbench.colorCustomizations": {
        "[Solarized Dark]": {
            "editorCursor.foreground": "#aeafad",
            "terminal.ansiBlack": "#000000",
            "terminal.ansiRed": "#cd3131",
            "terminal.ansiGreen": "#0dbc79",
            "terminal.ansiYellow": "#e5e510",
            "terminal.ansiBlue": "#2472c8",
            "terminal.ansiMagenta": "#bc3fbc",
            "terminal.ansiCyan": "#11a8cd",
            "terminal.ansiWhite": "#e5e5e5",
            "terminal.ansiBrightBlack": "#666666",
            "terminal.ansiBrightRed": "#f14c4c",
            "terminal.ansiBrightGreen": "#23d18b",
            "terminal.ansiBrightYellow": "#f5f543",
            "terminal.ansiBrightBlue": "#3b8eea",
            "terminal.ansiBrightMagenta": "#d670d6",
            "terminal.ansiBrightCyan": "#29b8db",
            "terminal.ansiBrightWhite": "#e5e5e5"
        },
    },
    "terminal.integrated.lineHeight": 1.2,
    "terminal.integrated.tabs.enabled": true,
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.defaultProfile.windows": "MSYS2",
    "terminal.integrated.profiles.windows": {
        "Git Bash": null,
        "PowerShell": null,
        "Windows PowerShell": null,
        "MSYS2": {
            "env": {
                "MSYSTEM": "MSYS",
                "CHERE_INVOKING": "1"
            },
            "path": "D:\\Software\\MSYS2\\usr\\bin\\bash.exe",
            "args": [
                "--login",
                "-i"
            ]
        }
    },
    // Extensions
    "remote.SSH.path": "D:\\Software\\OpenSSH\\ssh.exe",
    "remote.SSH.configFile": "C:\\Users\\Administrator\\AppData\\Roaming\\Code\\User\\ssh_config",
    "remote.SSH.remotePlatform": {
        "ITMP Docs": "linux"
    },
    // Sync ignored
    "git.path": "C:\\Users\\Administrator\\AppData\\Roaming\\Code\\User\\git-wrap.bat",
}
